{
	"name": "admin-ui",
	"version": "0.1.0",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "bun --hot src/index.ts",
		"typecheck": "tsc --noEmit",
		"build": "tsc --noEmit && bun build ./src/index.html --outdir=dist --sourcemap --target=browser --minify --define:process.env.NODE_ENV='\"production\"' --env='BUN_PUBLIC_*'",
		"start": "NODE_ENV=production bun src/index.ts",
		"lint": "eslint .",
		"lint:fix": "eslint . --fix",
		"format": "prettier --write .",
		"format:check": "prettier --check ."
	},
	"dependencies": {
		"@ant-design/icons": "^6.1.0",
		"antd": "^6.1.0",
		"i18next": "^25.2.1",
		"react": "^19",
		"react-dom": "^19",
		"react-i18next": "^15.5.2",
		"vetchium-specs": "file:../specs/typespec"
	},
	"devDependencies": {
		"@eslint/js": "^9.28.0",
		"@types/bun": "latest",
		"@types/react": "^19",
		"@types/react-dom": "^19",
		"eslint": "^9.28.0",
		"eslint-config-prettier": "^10.1.5",
		"eslint-plugin-react-hooks": "^5.2.0",
		"eslint-plugin-react-refresh": "^0.4.20",
		"globals": "^16.2.0",
		"prettier": "^3.7.4",
		"typescript": "~5.9.3",
		"typescript-eslint": "^8.33.1"
	}
}
