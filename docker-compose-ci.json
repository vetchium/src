			"environment": {
				"GARAGE_ADMIN_URL": "http://garage-usa1:3903",
				"GARAGE_ADMIN_TOKEN": "vetchium-dev-admin-token",
				"GARAGE_KEY_ID": "vetchium-dev-usa1-key",
				"GARAGE_KEY_SECRET": "vetchium-dev-usa1-secret-not-for-production",
				"GARAGE_BUCKET": "vetchium"
			}
		},
		"garage-init-deu1": {
