		"garage-init-global": {
			"image": "alpine:3.20",
			"depends_on": {
				"garage-global": { "condition": "service_healthy" }
			},
			"command": ["sh", "/garage/init-garage.sh"],
			"volumes": ["./garage/init-garage.sh:/garage/init-garage.sh:ro"],
			"environment": {
				"GARAGE_ADMIN_URL": "http://garage-global:3903",
				"GARAGE_ADMIN_TOKEN": "vetchium-dev-admin-token",
				"GARAGE_KEY_ID": "vetchium-dev-global-key",
				"GARAGE_KEY_SECRET": "vetchium-dev-global-secret-not-for-production",
				"GARAGE_BUCKET": "vetchium"
			}
		},
		"garage-init-ind1": {
			"image": "alpine:3.20",
			"depends_on": {
				"garage-ind1": { "condition": "service_healthy" }
			},
			"command": ["sh", "/garage/init-garage.sh"],
			"volumes": ["./garage/init-garage.sh:/garage/init-garage.sh:ro"],
			"environment": {
				"GARAGE_ADMIN_URL": "http://garage-ind1:3903",
				"GARAGE_ADMIN_TOKEN": "vetchium-dev-admin-token",
				"GARAGE_KEY_ID": "vetchium-dev-ind1-key",
				"GARAGE_KEY_SECRET": "vetchium-dev-ind1-secret-not-for-production",
