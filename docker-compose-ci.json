				"timeout": "5s",
				"retries": 10,
				"start_period": "10s"
			}
		},
		"garage-deu1": {
			"image": "dxflrs/garage:v2.2.0",
			"volumes": [
				"./garage/garage-deu1.toml:/etc/garage.toml:ro",
				"garage-deu1-meta:/var/lib/garage/meta",
				"garage-deu1-data:/var/lib/garage/data"
