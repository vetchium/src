			]
		},
		"seed-global": {
			"build": {
				"context": "./api-server",
				"dockerfile": "Dockerfile.seed"
			},
			"depends_on": {
				"migrate-global": {
					"condition": "service_completed_successfully"
				}
			},
			"command": [
				"postgres://vetchium:vetchium_dev@global-db:5432/vetchium_global?sslmode=disable",
				"-f",
				"/seed/global.sql"
			]
		},
